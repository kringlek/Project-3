    "hurricane_data = hurricane_data.loc[(hurricane_data[\"Damage\"]!=\"Extensive\") & (hurricane_data[\"Damage\"]!=\"[11][100][101][102]\") & (hurricane_data[\"Damage\"]!=\"[108][109]\") & (hurricane_data[\"Damage\"]!=\"[131][132][133]\")  & (hurricane_data[\"Damage\"]!=\"Minimal\")  & (hurricane_data[\"Damage\"]!=\"[142]\")  & (hurricane_data[\"Damage\"]!=\"[159][160][161][162][163][164][165][166][167]\")  & (hurricane_data[\"Damage\"]!=\"[144]\")  & (hurricane_data[\"Damage\"]!=\"[174]\") & (hurricane_data[\"Damage\"]!=\"Unknown\")  & (hurricane_data[\"Damage\"]!=\"Heavy\") & (hurricane_data[\"Damage\"]!=\"Minor\")  & (hurricane_data[\"Damage\"]!=\"Moderate\") & (hurricane_data[\"Damage\"]!=\"Millions\") & (hurricane_data[\"Damage\"]!=\"13\") & (hurricane_data[\"Damage\"]!=\"5\") & (hurricane_data[\"Damage\"]!=\"None\")]\n",
